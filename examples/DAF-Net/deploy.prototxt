#######################
layer {
   bottom: "conv2_group_1x1"
   bottom: "attention_weight64"
   top: "attentional_feature"
   name: "attentional_feature"
   type: "EltwiseAdv"
   eltwise_param {
     operation: PROD
   }
}
######################
layer {
  name: "attentional_conv1"
  type: "Convolution"
  bottom: "attentional_feature"
  top: "attentional_conv1"
  convolution_param {
    num_output: 192
    bias_term: false
    kernel_size: 1
    stride: 1
    group: 64    #combine the 256/64 channels in each group into 3 channel (3*64=192)
    weight_filler {
      type: "msra"
    }
  }
}

layer {
  name: "relu_attentional_conv1"
  type: "ReLU"
  bottom: "attentional_conv1"
  top: "attentional_conv1"
}

layer {
  name: "attentional_conv2"
  type: "Convolution"
  bottom: "attentional_conv1"
  top: "attentional_conv2"
  convolution_param {
    num_output: 192
    kernel_size: 3
    pad: 1
    stride: 1
    group: 64   
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}


layer {
  name: "attentional_conv3"
  type: "Convolution"
  bottom: "attentional_conv2"
  top: "attentional_conv3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 3  #summarize the 64 rain streak layers 
    bias_term: false
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}


layer {
  name: "norm_residual"
  bottom: "attentional_conv3"
  top: "norm_residual"
  type: "TanH"
}

layer {
    name: "output1"
    type: "Eltwise"
    bottom: "data"
    bottom: "norm_residual"
    top: "output1"
    eltwise_param {
        operation: SUM
    }
}

layer {
  name: "loss1"
  type: "EuclideanLoss"
  bottom: "output1"
  bottom: "label"
  top: "loss1"
  loss_weight: 1
  include {
    phase: TRAIN
  }
}
