layer {
  name: "conv1_group_1x1"
  type: "Convolution"
  bottom: "conv2-dsn1"
  top: "conv1_group_1x1"
  convolution_param {
    num_output: 256  
    bias_term: false
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "msra"
    }
  }
}

layer {
  name: "relu1_group_1x1"
  type: "ReLU"
  bottom: "conv1_group_1x1"
  top: "conv1_group_1x1"
}


layer {
  name: "conv1_group_3x3"
  type: "Convolution"
  bottom: "conv1_group_1x1"
  top: "conv1_group_3x3"
  convolution_param {
    num_output: 256
    bias_term: false
    pad: 1
    kernel_size: 3
    
    stride: 1
    weight_filler {
      type: "msra"
    }
  }
}

layer {
  name: "relu1_group_3x3"
  type: "ReLU"
  bottom: "conv1_group_3x3"
